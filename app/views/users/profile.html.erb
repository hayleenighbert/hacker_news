<div class="container">
	<div class="row">
		<div class="col-sm-8 col-sm-offset-2">
			<h1><%= @current_user.name %>
			<br>Links</h1>
		</div>
	</div>
</div>
	<div class="row">
		<div class="col-sm-8 col-sm-offset-2">
	<% @current_user.post.each do |post| %>
		<ul>
		<li>
		<%= link_to post.title, post.link %>
		</li>
		</ul>
	<% end %>	

</div>
	</div>